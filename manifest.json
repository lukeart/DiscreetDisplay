{
    "manifest_version": 2,
    "name": "DiscreetDisplay",
    "version": "1.0",
    "description": "Blurs specific elements in web pages.",
    "permissions": ["activeTab", "storage"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
      }
    },
    "web_accessible_resources": ["config.json"]
  }
  