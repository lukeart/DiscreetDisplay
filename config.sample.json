{
  "rules": [
    {
      "type": "elementSelector",
      "parentSelector": ".content-area",
      "childSelectors": [
        {
          "selector": ".sensitive-info",
          "method": "blur",
          "level": "5px"
        },
        {
          "selector": ".confidential-text",
          "method": "scramble"
        }
      ]
    },
    {
      "type": "tableColumn",
      "tableSelector": ".data-table",
      "columnNameSelector": "th",
      "rowSelector": "tr",
      "cellSelector": "td",
      "columnIdentifier": {
        "method": "headerName",
        "value": "Email"
      },
      "hidingMethod": "pixelate",
      "level": "medium"
    },
    {
      "type": "tableColumn",
      "tableSelector": ".report-table",
      "columnNameSelector": "th",
      "rowSelector": "tr",
      "cellSelector": "td",
      "columnIdentifier": {
        "method": "index",
        "value": 2
      },
      "hidingMethod": "blur",
      "level": "3px"
    }
  ]
}
